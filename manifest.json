{
  "manifest_version": 3,
  "name": "Tab Groups Export/Import",
  "version": "1.5.0",
  "description": "Export open tab groups to JSON and import them into another Chromium browser.",
  "permissions": ["tabs", "tabGroups", "downloads", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_title": "Tab Groups Export/Import",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "service-worker.js"
  }
}
